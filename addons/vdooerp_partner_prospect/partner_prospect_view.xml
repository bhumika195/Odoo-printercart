<?xml version="1.0"?>
<openerp>
    <data>

        <record id="view_res_partner_prospect_form" model="ir.ui.view">
            <field name="name">res.partner.prospect.form.view</field>
            <field name="model">res.partner</field>
            <field name="inherit_id" ref="base.view_partner_form"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='email']" position="after">
                    <field name="prospect"/>
                  <field name="inactive_customer"/>
                </xpath>
            </field>
        </record>

    </data>
</openerp>
